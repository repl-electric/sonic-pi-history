
live_audio "moog",input: 2

live_loop :lead do
  tick
  smp Mountain[/subkick/,0], amp: 10+rand
  form chord(:fs4,:M)[0..1],1*(line 1,128,128).look, sustain: 1.0
  sleep 1
end

live_loop :bass, sync: :lead do
  tick
  moog (ring :Fs1, :Gs1, :A1, :Gs1, :A1, :B1, :Cs2).look, sustain: 3.9
  sleep 4
end
