live_loop(:metro)do
  tick;
  cue("32") if look % 32 == 0
  cue("64") if look % 64 == 0
  puts "ARE YOU ALIVE"
  sleep 1
end


live_loop :pattern, sync: :metro do
  tick
  #sync "32"
  c1 = Dust[/clap/,1]
  k1,k2 = Frag[/kick/,9], Dust[/kick/,2]
  s=(ring *%W{k1 _ _ _   k2 _ _ _   c1 _ _ _   k1 _ _ _}).look
  smp eval(s)

  sleep 1/8.0
end
