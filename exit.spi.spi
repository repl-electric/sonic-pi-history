REST=[[[_, 0.25]], 2];use_bpm 63.5
live_loop :Fall_out_of_your_screen, sync: :unsyncable do tick;
  live_loop :Within_the_letters{stop}
  unity "/camera/4"

  unity "/logo/person", 0.0

  score =  (ing  #15.25
    [[[:b3 2]  [:d3 2]   [:fs3 2]]           2]                  REST
    [[[:fs3 2] [:a3 2]   [:cs4 2]]           2]                  REST
    [[[:a3 2]  [:cs3 2]   [:e4 2]]            2]
    [[[:d3 2]  [:fs4 2]  [:a3 2]  [[:cs5 2] [:e4 2]].choose] 2]  REST
    [[[:e3 2]  [:gs3 2]  [:b3 2]]  2] REST
    [[[:fs3 2] [:a3 2]   [:cs3 2] ]  2]                  REST
#    [[[:fs4 2] [:A4 2]   [:Cs4 2] ]  2]                          REST
    )
  cue :kernel_panic
  s=score.look;;c=s[0];zzz=s[-1]
  #whitespace c[0][0], sus: 12
  if spread(1,6).look
    #rev :b5, 1
    #eek :Gs3, 10
  end
  #rev :a6, 15, sus: 12
  if !c[0][0]
    #eek (ing :e4 :gs4 :e4 :b4 _ _).look, (ing 100 120).look(:missing), sus: 3.0, at: 0.0, mod: 0.0, mode: 2
  end
  with_transpose -12 {cpu2 c[1], 60, sus: 2 }
  c.each_tick(:inner){|n|
    qbitsea n,2.0*(ing 80 70 85 70).look(:inner), mode: 0, cutoff: 1.0, phase: 0, comp: 0
  }

  pf=2.3
  i=(method :operator)
  at{
    i.call(c[0], 60*pf, sus: 4, off: true)
    sleep 0.5
    i.call(c[1], 50*pf, sus: 4, off: true)
    sleep 0.25
    i.call(c[2], 55*pf, sus: 4, off: true)
        sleep 0.25
    i.call(c[3], 70*pf, sus: 2)
        sleep 0.25
    i.call(c[4], 30*pf, sus: 8)
        sleep 0.25
    i.call(c[5], 30*pf, sus: 8)
        sleep 0.25
    }

  exception root(c), 127, sus: 2, atk: 0.1, shape: 1.0  #, wet: 0.1, more: 0.0, shape: 0.8, atk: 0.2
  null root(c), 127*0.8, mode: 2

  8.times{tick(:in2);
    callstack (ing
          :a5 _ :a4 _    _   :b5  :a4 _
          :a5 _ :a4 _    _   :b5  :a4 _
          :a5 _ :fs5 _   _   :fs5 :a4 _
          :a5 _ :e5 _    _   :cs6 :a4 _
          :a5 _ :a4 _   :e5  :b5  :a4 _
          :a5 _ :a4 _    _   :b5  :a4 _
          :a5 _ :a4 _    _   :b5  :a4 _
          :a5 _ :a4 _    _   :b5  :a4 _
          :a5 _ :a4 _    _   :b5  :a4 _
          :a5 _ :a4 _    _   :b5  :a4 _
          :a5 _ :a4 _    _   :b5  :a4 _
          :a5 _ :a4 _    :e6 :b5  :a4 _
          ).look(:in2), (ing 90 90 80 80 70 70 60 60).look(:in2)*0
    sleep zzz/8.0
  }
end

no{
qbitsea_cc oct: 0.68
qbitsea_cc drive: 0.44
qbitsea_cc charge: 0.52
qbitsea_cc fm: 0.70
qbitsea_cc wav: 1.00
qbitsea_cc wav_amp: 0.25
qbitsea_cc atk: 0.10
qbitsea_cc mod: 0.80
qbitsea_cc motion: 0.036
qbitsea_cc formant: 0.50
qbitsea_cc mul: 0.00
qbitsea_cc bass: 0.61

#qbitsea_cc width: 0.50
#qbitsea_cc center: 0.15
#qbitsea_cc head: 0.00

eq lo: 0.85
eq mi: 0.85
eq hi: 0.85
}
