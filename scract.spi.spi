def eek(*args)
  params, opts = split_params_and_merge_opts_array(args)
  n, vel = *params

  puts n
  puts vel
  puts opts
  if n.is_a?(Array)
    opts =  opts  << {sustain: n[1]}
    n = n[0]
  end

  midi [n,10], 10, *opts
end



eek [(ring :A2, :Cs3).tick(:in),10], 120, sustain: 1, attack: 1.0 if spread(1,16).look

