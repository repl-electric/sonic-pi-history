live_loop :git do
  tick
  midi (ring :Cs3).look, channel: 3, port: :iac_bus_1, sustain: 3
  sleep 3
end

live_loop :high, sync: :git do
  tick
  sharp (ring :A3, :Cs4, :E4, _, _).look, sustain: 12
  sleep 12
end




