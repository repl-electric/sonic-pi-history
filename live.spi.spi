    #stop
    zs=(ing
      :d4 :cs4 _ :a3 _ :e4 _ :a4 :a4 :a4 :a4 :a4
      :d4 :cs4 _ :a3 _ :b3 _ :a4 :a4 :a4 :fs4 :a4
      )
