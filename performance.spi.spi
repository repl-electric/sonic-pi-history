REST=[_]
#mode0

bitsea_cc motion: 0.16
bitsea_cc octave: 0.50
#bitsea_cc formant: 0.00
bitsea hi: 0.90

live_loop :hi do
  mode2
  tick
  score=(ing
    [:b2 :d5 :fs3]
    [:fs2 :a4 :cs3]
    ).look

  score.each_tick(:in){|n|
    bitsea n, 1.0*(ing 80 70 85 70).look(:in), mode: 0, cutoff: 1.0
  }



  exception :A2, sus: 0.125
  8.times{
    kick_machine (ing K1 _ _ _).look
    sleep 2.0/8.0
  }
end
