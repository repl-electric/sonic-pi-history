notes = (ring :E4, :Fs4, :B4, :Cs5, :D5, :Fs4, :E4, :Cs5, :B4, :Fs4, :D5, :Cs5)

live_loop :slow do
  #use_random_seed 300

  use_synth [:beep, :dsaw, :prophet].choose


  play notes.tick, release: 0.5, cutoff: 80 #if rand > 0.5
  sleep 0.3
end

live_loop :faster do
  play notes.tick, release: 0.25
  sleep 0.295
end