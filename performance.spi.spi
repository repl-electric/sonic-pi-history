#mode0

#bitsea_on :cs4
bitsea_cc motion: 0.15
bitsea_cc octave: 0.50
bitsea_cc formant: 0.50

live_loop :go do
  tick
  mode4
  s=(ing
    [:b2  :d5 :fs3]
    [:fs2 :a4 :cs3]
    ).look
  s.each_tick(:in){|n|
    qbitsea n, 1.0*(ing 80 70 85 70).look(:in), mode: 0, cutoff: 1.0
  }
  exception (ing s[0] _).look

  2.times{sleep 1; kick_machine K1}
end
