live_loop :testtest do
  tick
  score = (ring [:fs1, 1], [:fs1,1], [:gs1,2], [:a1,2], [:gs1,2], [:a1,1], [:a1,1], [:b1,1], [:cs2,2]).look
  moog score[0], 100, sustain: score[1]-0.05
  sleep score[1]
end

live_loop :drums, sync: :testtest do
  tick
  if (ring *%W{1 0 0 0   1 0 0 0}).look == "1"
    smp Mountain[/subkick/,0], amp: 2
  end
  sleep 1/4.0
end
