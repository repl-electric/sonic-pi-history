live_loop :name do
  tick
  moog (ring :E1, :Fs1, :D1).look, sustain: 2.0
  sleep 3
end

live_loop :kick, sync: :name do
  tick
  if spread(1,4).look
    smp Dust[/kick/,3], amp: 4
  end

  if (s=(ring *%W{_ _ h1 _})) != _
    smp Dust[/hat/,0]
  end

  h1 = Dust[/hat/,3]
  if (s=(ring *%W{
_ _ h1 _
_ _ h1 _
_ _ h1 {path:h1,:amp 0.2}
_ _ h1 _
_ _ h1 _

})) != _
    smp eval(s)
  end



  sleep 1/4.0
end
